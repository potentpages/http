{
  "name": "potentpages/http",
  "type": "library",
  "description": "Simple async library for making HTTP requests",
  "keywords": [
    "http requests",
    "curl",
    "multi",
    "rest",
    "client",
    "http"
  ],
  "homepage": "https://potentpages.com",
  "autoload": {
    "psr-4": {
      "PotentHTTP\\": "src/"
    }
  },
  "require": {
    "php": ">=7.1",
    "ext-curl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7"
  },
  "autoload-dev": {
    "psr-4": {
      "PotentHTTP\\Tests\\": "tests/"
    }
  }
}
